    },
    {
      "id": "CVE-2025-55315",
      "problem": ".NET Security Feature Bypass Vulnerability",
      "severity": "critical",
      "timeline": {
        "disclosed": "2025-10-14",
        "fixed": "2025-10-14"
      },
      "cvss": {
        "version": "3.1",
        "vector": "CVSS:3.1/AV:N/AC:L/PR:L/UI:N/S:C/C:H/I:H/A:L/E:U/RL:O/RC:C"
      },
      "description": [
        "Inconsistent interpretation of http requests ('http request/response smuggling') in ASP.NET Core allows an authorized attacker to bypass a security feature over a network."
      ],
      "platforms": [
        "all"
      ],
      "architectures": [
        "all"
      ],
      "cna": "microsoft",
      "references": [
        "https://github.com/dotnet/announcements/issues/371"
      ]
    },
    {
      "id": "CVE-2025-55247",
      "problem": ".NET Denial of Service Vulnerability",
      "severity": "high",
      "timeline": {
        "disclosed": "2025-10-14",
        "fixed": "2025-10-14"
      },
      "cvss": {
        "version": "3.1",
        "vector": "CVSS:3.1/AV:L/AC:L/PR:L/UI:R/S:U/C:H/I:H/A:H/E:U/RL:O/RC:C"
      },
      "description": [
        "A vulnerability exists in .NET where predictable paths for MSBuild's temporary directories on Linux let another user create the directories ahead of MSBuild, leading to DoS of builds. This only affects .NET on Linux operating systems."
      ],
      "platforms": [
        "linux"
      ],
      "architectures": [
        "all"
      ],
      "cna": "microsoft",
      "references": [
        "https://github.com/dotnet/announcements/issues/370"
      ],
      "mitigation": [
        "Projects which do not utilize the DownloadFile build task are not susceptible to this vulnerability."
      ]
    }
